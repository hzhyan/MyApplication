<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hyan">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-feature android:glEsVersion="0x00020000" android:required="true" />

    <application
        android:name="com.example.hyan.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.example.hyan.ActivityMain"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.hyan.resourceupdate.ActivityResourceUpdate"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.example.hyan.rxjavatest.ActivityRxJavaTest"
            android:screenOrientation="portrait" />

        <activity
            android:name=".customprogressbar.ActivityCustomProgressBar"
            android:screenOrientation="portrait" />

        <activity
            android:name=".retrofit.ActivityRetrofitTest"
            android:screenOrientation="portrait" />

        <activity
            android:name=".opengl.ActivityOpenGL"
            android:screenOrientation="portrait" />

        <activity
            android:name=".openglstl.ActivityOpenGLSTL"
            android:screenOrientation="portrait" />

        <activity
            android:name=".animation.ActivityAnimation"
            android:screenOrientation="portrait" />

        <activity
            android:name=".openglshader.ActivityShader"
            android:screenOrientation="portrait" />

    </application>


</manifest>
